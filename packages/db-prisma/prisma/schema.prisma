generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Post {
  id      String @id
  title   String
  content String
}

model drizzle_migrations {
  id         BigInt  @id @unique(map: "id") @default(autoincrement()) @db.UnsignedBigInt
  hash       String  @db.Text
  created_at BigInt?

  @@map("__drizzle_migrations")
}

model cities {
  id         BigInt             @id @unique(map: "id") @default(autoincrement()) @db.UnsignedBigInt
  name       String?            @db.VarChar(256)
  country_id Int?
  popularity cities_popularity?
}

model countries {
  id   BigInt  @id @unique(map: "id") @default(autoincrement()) @db.UnsignedBigInt
  name String? @unique(map: "name_idx") @db.VarChar(256)
}

enum cities_popularity {
  unknown
  known
  popular
}
